#-----------------------------------------------------------------------------
#
# guiConfig.cmake - CMake configuration file for external projects.
# This file was automatically generated by the cmake macro 
# CREA_INSTALL_LIBRARY_FOR_CMAKE of the package CREA
#
# This file is configured by cmake and used by the
# Usegui.cmake module to load the lib settings 
# for an external project.

# Build tree config ?
SET(CILC_BUILD_TREE_CONFIGURATION FALSE)


IF(UNIX)
SET(GOTO_INSTALL_PREFIX /../../..)
ENDIF(UNIX)


# The gui include file *RELATIVE* directories.
SET(CILC_RELATIVE_INCLUDE_PATHS "include/gui")
# Compute the prefix for include and library paths
IF(CILC_BUILD_TREE_CONFIGURATION)
  # In build tree 
  # the include paths are relative to the source tree *AND* the binary tree 
  # for generated files 
  SET(CILC_INCLUDE_PATH_PREFIX /work/ltu/Correspondence_Oct7/Pablo2_Oct7)
  # Build the *ABSOLUTE* directories
  FOREACH(path ${CILC_RELATIVE_INCLUDE_PATHS})
    SET(gui_INCLUDE_DIRS
      ${gui_INCLUDE_DIRS}
      ${CILC_INCLUDE_PATH_PREFIX}/${path}
      )
  ENDFOREACH(path ${CILC_RELATIVE_INCLUDE_PATHS})
  SET(CILC_INCLUDE_PATH_PREFIX /work/ltu/Correspondence_Oct7/binPablo2_Oct7)
  # Build the *ABSOLUTE* directories
  FOREACH(path ${CILC_RELATIVE_INCLUDE_PATHS})
    SET(gui_INCLUDE_DIRS
      ${gui_INCLUDE_DIRS}
      ${CILC_INCLUDE_PATH_PREFIX}/${path}
      )
  ENDFOREACH(path ${CILC_RELATIVE_INCLUDE_PATHS})
ELSE(CILC_BUILD_TREE_CONFIGURATION)
  # In install tree 
  # the include paths are relative to install prefix 
  # On unix , GOTO_INSTALL_PREFIX allows to get back to the 
  # installation prefix from  gui_DIR
  SET(CILC_INCLUDE_PATH_PREFIX ${gui_DIR}${GOTO_INSTALL_PREFIX})
  # Build the *ABSOLUTE* directories
  FOREACH(path ${CILC_RELATIVE_INCLUDE_PATHS})
    SET(gui_INCLUDE_DIRS
      ${gui_INCLUDE_DIRS}
      ${CILC_INCLUDE_PATH_PREFIX}/${path}
      )
  ENDFOREACH(path ${CILC_RELATIVE_INCLUDE_PATHS})
ENDIF(CILC_BUILD_TREE_CONFIGURATION)



# Compute the prefix for library paths
IF(CILC_BUILD_TREE_CONFIGURATION)
  # In build tree
  # the library paths are relative to the binary tree 
  SET(CILC_LIBRARY_PATH_PREFIX /work/ltu/Correspondence_Oct7/binPablo2_Oct7)
ELSE(CILC_BUILD_TREE_CONFIGURATION)
  # In install tree 
  # the library paths are relative to install prefix
  SET(CILC_LIBRARY_PATH_PREFIX ${gui_DIR}${GOTO_INSTALL_PREFIX})
ENDIF(CILC_BUILD_TREE_CONFIGURATION)
# The gui library file *RELATIVE* directories.
SET(CILC_RELATIVE_LIBRARY_PATHS "lib64")
# Build the *ABSOLUTE* directories
FOREACH(path ${CILC_RELATIVE_LIBRARY_PATHS})
  SET(gui_LIBRARY_DIRS
    ${gui_LIBRARY_DIRS}
    ${CILC_LIBRARY_PATH_PREFIX}/${path}
    )
ENDFOREACH(path ${CILC_RELATIVE_LIBRARY_PATHS})

# Set the "prefix path"
SET(gui_INSTALL_PREFIX ${CILC_LIBRARY_PATH_PREFIX})

# The C and C++ flags added by gui to the cmake-configured flags.
SET(gui_REQUIRED_C_FLAGS "")
SET(gui_REQUIRED_CXX_FLAGS "")
SET(gui_REQUIRED_LINK_FLAGS "")

# The gui version 
SET(gui_MAJOR_VERSION )
SET(gui_MINOR_VERSION )
SET(gui_BUILD_VERSION )
SET(gui_VERSION ..)

# The location of the Usegui.cmake file.
SET(gui_USE_FILE "${gui_DIR}/Usegui.cmake")

# The build settings file.
SET(gui_BUILD_SETTINGS_FILE 
  "${gui_DIR}/guiBuildSettings.cmake")

# A list of all libraries for gui.  Those listed here should
# automatically pull in their dependencies.
SET(gui_LIBRARIES gui)

# Messages
IF(CREA_VERBOSE_CMAKE)
  MESSAGE(STATUS "=======================================")
  MESSAGE(STATUS "Looking for gui... found:")
  MESSAGE(STATUS "* gui_DIR          = ${gui_DIR}")
  MESSAGE(STATUS "* gui_VERSION      = ${gui_VERSION}")
  MESSAGE(STATUS "* gui_USE_FILE     = ${gui_USE_FILE}")

  MESSAGE(STATUS "* gui_INCLUDE_DIRS = ${gui_INCLUDE_DIRS}")
  MESSAGE(STATUS "* gui_LIBRARY_DIRS = ${gui_LIBRARY_DIRS}")
  MESSAGE(STATUS "* gui_LIBRARIES    = ${gui_LIBRARIES}")
ENDIF(CREA_VERBOSE_CMAKE)

# Does the library has an additional config file (user provided) ?
SET(gui_HAS_ADDITIONAL_CONFIG_FILE FALSE)

IF (gui_HAS_ADDITIONAL_CONFIG_FILE)
  IF(CREA_VERBOSE_CMAKE)
    MESSAGE(STATUS "Reading gui additional configuration file")
  ENDIF(CREA_VERBOSE_CMAKE)
  # Include it
  INCLUDE(${gui_DIR}/AdditionalguiConfig.cmake)
ENDIF (gui_HAS_ADDITIONAL_CONFIG_FILE)
